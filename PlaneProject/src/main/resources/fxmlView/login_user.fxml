<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hu.adatb.view.controller.LoginUserController">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="151.0" minWidth="10.0" prefWidth="94.0" />
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="228.0" minWidth="10.0" prefWidth="218.0" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints maxHeight="50.0" minHeight="10.0" prefHeight="50.0" vgrow="SOMETIMES" />
        <RowConstraints maxHeight="88.0" minHeight="10.0" prefHeight="63.0" vgrow="SOMETIMES" />
        <RowConstraints maxHeight="91.0" minHeight="5.0" prefHeight="64.0" vgrow="SOMETIMES" />
        <RowConstraints maxHeight="139.0" minHeight="10.0" prefHeight="43.0" vgrow="SOMETIMES" />
        <RowConstraints maxHeight="116.0" minHeight="10.0" prefHeight="116.0" vgrow="SOMETIMES" />
    </rowConstraints>
    <children>
        <Label text="Név" GridPane.rowIndex="1">
         <font>
            <Font size="13.0" />
         </font></Label>
        <Label text="Jelszó" GridPane.rowIndex="2">
         <font>
            <Font size="13.0" />
         </font></Label>
        <TextField fx:id="nameField" promptText="Add meg a felhasznalóneved" GridPane.columnIndex="1" GridPane.rowIndex="1">
         <font>
            <Font size="13.0" />
         </font></TextField>
        <PasswordField fx:id="passwordField" promptText="Add meg a jelszavad" GridPane.columnIndex="1" GridPane.rowIndex="2">
         <font>
            <Font size="13.0" />
         </font></PasswordField>
        <Button cancelButton="true" mnemonicParsing="false" onAction="#cancel" style="-fx-background-color: #a90707;" text="Mégse" textAlignment="CENTER" textFill="WHITE" GridPane.halignment="CENTER" GridPane.rowIndex="4" GridPane.valignment="CENTER">
            <opaqueInsets>
                <Insets />
            </opaqueInsets>
         <font>
            <Font size="14.0" />
         </font>
        </Button>
        <Button fx:id="loginButton" defaultButton="true" mnemonicParsing="false" onAction="#login" style="-fx-background-radius: 10; -fx-background-color: #1656d6;" text="Bejelentkezés" textFill="WHITE" GridPane.columnIndex="1" GridPane.halignment="LEFT" GridPane.rowIndex="4" GridPane.valignment="CENTER">
         <font>
            <Font size="16.0" />
         </font></Button>
        <ImageView fitHeight="47.0" fitWidth="126.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../pictures/loginPic.png" />
            </image>
        </ImageView>
    </children>
    <opaqueInsets>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </opaqueInsets>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</GridPane>
